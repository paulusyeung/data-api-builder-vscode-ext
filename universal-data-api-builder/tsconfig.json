{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2020",
        "outDir": "out",
        "lib": [
            "es2020"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test",
        "webview-ui",
        "vite.config.ts"
    ]
}